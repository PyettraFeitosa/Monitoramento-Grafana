      "targets": [
        {
          "dataset": "checkout",
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH base AS (\n    SELECT\n        (\n            TIMESTAMP '2024-01-01'\n            + (replace(hour, 'h', '') || ' hours')::interval\n        ) AS time,\n        today,\n        avg_last_week,\n        yesterday,\n        same_day_last_week,\n        source_file\n    FROM checkout_sales_hourly\n    WHERE source_file = 'checkout_1'\n),\nexpanded AS (\n    SELECT time, today, avg_last_week, yesterday AS val FROM base\n    UNION ALL\n    SELECT time, today, avg_last_week, same_day_last_week FROM base\n    UNION ALL\n    SELECT time, today, avg_last_week, avg_last_week FROM base\n),\nstats AS (\n    SELECT\n        time,\n        MAX(today) AS today,\n        MAX(avg_last_week) AS avg_last_week,\n        STDDEV_POP(val) AS stddev\n    FROM expanded\n    GROUP BY time\n)\nSELECT\n    time,\n    today,\n    avg_last_week,\n    avg_last_week + 2 * stddev AS anomaly_threshold\nFROM stats\nORDER BY time;\n",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          }
        }
      ],
